   "source": [
    "# Выполняем запрос по заданию\n",
    "connection = sqlite3.connect('C:.......chinook.db')\n",
    "cursor = connection.cursor()\n",
    "\n",
    "query = '''\n",
    "SELECT\n",
    "  CLIENTS_TABLE.GENDER,\n",
    "  COUNT(CLIENTS_TABLE.CLIENT_ID) filter (WHERE CLIENTS_TABLE.CLIENT_ID IN \n",
    "      (SELECT CLIENT_ID from LOANS_TABLE GROUP BY CLIENT_ID HAVING COUNT(*) = 1)) \n",
    "      as 'Количество 1 договоров, оформленных в 2020',\n",
    "  COUNT(CLIENTS_TABLE.CLIENT_ID) filter (WHERE CLIENTS_TABLE.CLIENT_ID IN \n",
    "      (SELECT CLIENT_ID from LOANS_TABLE GROUP BY CLIENT_ID HAVING COUNT(*) = 2)) \n",
    "      as 'Количество 2 договоров, оформленных в 2020',\n",
    "  COUNT(CLIENTS_TABLE.CLIENT_ID) filter (WHERE CLIENTS_TABLE.CLIENT_ID IN \n",
    "      (SELECT CLIENT_ID from LOANS_TABLE GROUP BY CLIENT_ID HAVING COUNT(*) = 3)) \n",
    "      as 'Количество 3 договоров, оформленных в 2020',\n",
    "  COUNT(CLIENTS_TABLE.CLIENT_ID) filter (WHERE CLIENTS_TABLE.CLIENT_ID IN \n",
    "      (SELECT CLIENT_ID from LOANS_TABLE GROUP BY CLIENT_ID HAVING COUNT(*) = 4)) \n",
    "      as 'Количество 4 договоров, оформленных в 2020'\n",
    "FROM LOANS_TABLE JOIN CLIENTS_TABLE\n",
    "WHERE LOANS_TABLE.CLIENT_ID = CLIENTS_TABLE.CLIENT_ID\n",
    "GROUP BY CLIENTS_TABLE.GENDER\n",
    "\n",
    "'''\n",
    "df = pd.read_sql_query(query, connection)   \n",
    "df"
   ]
